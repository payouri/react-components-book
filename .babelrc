{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "corejs": {
                    "version": "3.8",
                    "proposals": true
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-regenerator" /*,
        "@babel/plugin-syntax-dynamic-import" */
    ]
}